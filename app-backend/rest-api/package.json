{
  "name": "serverless-graphql-apollo",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:serverless/serverless-graphql-apollo.git"
  },
  "scripts": {
    "test": "jest",
    "start-client-local": "cd ../../app-client/ && env-cmd ../config/security.env.local yarn start",
    "start-client-prod": "cd ../../app-client/ && env-cmd ../config/security.env.prod yarn start",
    "restapi-start-server-lambda:offline": "env-cmd ../../config/security.env.local serverless offline start",
    "restapi-deploy-server-lambda-prod": "serverless --stage=production deploy"
  },
  "dependencies": {
    "apollo-server-lambda": "1.2.0",
    "apollo-server-module-graphiql": "1.2.0",
    "babel-polyfill": "^6.26.0",
    "graphql": "^0.10.5",
    "graphql-playground-middleware": "^1.1.0",
    "graphql-tools": "2.7.2",
    "node-fetch": "^1.7.3",
    "oauth": "^0.9.15"
  },
  "devDependencies": {
    "aws-sdk": "^2.4.8",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.15.2",
    "cors": "2.8.4",
    "env-cmd": "^5.0.0",
    "imports-loader": "^0.7.1",
    "jest": "^21.2.1",
    "serverless-offline": "^3.15.3",
    "serverless-webpack": "^4.1.0",
    "webpack": "^3.8.1",
    "webpack-node-externals": "^1.6.0"
  }
}
